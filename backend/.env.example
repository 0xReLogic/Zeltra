# Environment variables for Zeltra
# Copy this to .env and fill in your values

# Run mode: development, staging, production
RUN_MODE=development

# Database
ZELTRA__DATABASE__URL=postgres://postgres:postgres@localhost:5432/zeltra_dev

# JWT
ZELTRA__JWT__SECRET=your-secret-key-here

# Logging
RUST_LOG=zeltra=debug,tower_http=debug
